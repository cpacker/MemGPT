{
	"printWidth": 120,
	"useTabs": true,
	"tabWidth": 2,
	"trailingComma": "es5",
	"semi": true,
	"singleQuote": true,
	"bracketSpacing": true,
	"arrowParens": "always",
	"jsxSingleQuote": false,
	"bracketSameLine": false,
	"endOfLine": "lf",
	"plugins": ["prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"]
}
